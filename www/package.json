{
	"name": "gooey-www",
	"version": "0.0.1",
	"private": true,
	"description": "Website for [gooey](https://github.com/braebo/gooey).",
	"keywords": [
		"sveltekit",
		"template",
		"starter",
		"svelte",
		"kit"
	],
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'element_invalid_self_closing_tag:ignore'",
		"check:watch": "svelte-kit sync && svelte-check --watch --tsconfig ./tsconfig.json --compiler-warnings 'element_invalid_self_closing_tag:ignore'",
		"dev": "vite dev",
		"extras": "pnpm i -D type-fest froebel nanostores rxjs",
		"format": "prettier --write --plugin-search-dir=. .",
		"postinstall": "svelte-kit sync",
		"lint": "prettier --check . --plugin-search-dir .",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@playwright/test": "1.45.1",
		"@shikijs/transformers": "1.10.3",
		"@sveltejs/adapter-auto": "3.2.2",
		"@sveltejs/kit": "2.5.18",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/cookie": "0.6.0",
		"@types/node": "20.14.10",
		"autoprefixer": "10.4.19",
		"cookie": "0.6.0",
		"cssnano": "7.0.4",
		"esm-env": "1.0.0",
		"fractils": "3.1.3",
		"gooey": "link:..",
		"greset": "2.0.0",
		"mdsvex": "0.11.2",
		"postcss": "8.4.39",
		"postcss-load-config": "6.0.1",
		"prettier": "3.3.2",
		"prettier-plugin-svelte": "3.2.5",
		"pug": "3.0.3",
		"rehype-autolink-headings": "7.1.0",
		"rehype-slug": "6.0.0",
		"remark-abbr": "1.4.2",
		"sass": "1.77.6",
		"shiki": "1.10.3",
		"svelte": "4.2.18",
		"svelte-awesome-pug": "0.1.3",
		"svelte-check": "3.8.4",
		"svelte-preprocess": "6.0.2",
		"tslib": "2.6.3",
		"typescript": "5.5.3",
		"vite": "5.3.3",
		"vitest": "2.0.1"
	}
}
